{
  "editor.formatOnSave": false,
  "editor.defaultFormatter": null,
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.useFlatConfig": true,
  "eslint.validate": ["javascript", "typescript", "vue"],
  "files.associations": {
    "*.css": "tailwindcss"
  }
}
